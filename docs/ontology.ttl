:Zaragoza rdf:type owl:NamedIndividual ,
                   :City ;
          :identifier "5" ;
          :latLong "41.6488,-0.8891" ;
          rdfs:comment "A major Spanish city known for its rich history, landmarks, and dynamic urban life, attracting both tourists and businesses." ;
          rdfs:label "Zaragoza" .


#################################################################
#    Rules
#################################################################

<urn:swrl:var#h> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl:var#stars> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl:var#price> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl:var#r> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl:var#rating> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl:var#ta> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

[ rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
