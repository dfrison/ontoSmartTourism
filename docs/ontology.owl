    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/elements/1.1/abstract -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/abstract"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#sameAs -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#cityHasEvent -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#cityHasEvent">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#takesPlaceIn"/>
        <rdfs:comment>Object property to find all events in a city</rdfs:comment>
        <rdfs:label>City Has Event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#cityHasHotel -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#cityHasHotel">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#hotelIsLocatedIn"/>
        <rdfs:comment>Object property to find all hotels in a city</rdfs:comment>
        <rdfs:label>City Has Hotel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#cityHasRestaurant -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#cityHasRestaurant">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#restaurantIsLocatedIn"/>
        <rdfs:comment>Object property to find all restaurants in a city</rdfs:comment>
        <rdfs:label>City Has Restaurant</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#cityHasTouristAttraction -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#cityHasTouristAttraction">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#touristAttractionIsLocatedIn"/>
        <rdfs:comment>Object property to find all tourist attraction in a city</rdfs:comment>
        <rdfs:label>City has tourist attraction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#cityHasTransport -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#cityHasTransport">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#transportOperatesIn"/>
        <rdfs:comment>Object property to find all trasnports in a city</rdfs:comment>
        <rdfs:label>City Has Transport</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#hotelIsLocatedIn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#hotelIsLocatedIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Hotel"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#City"/>
        <rdfs:comment xml:lang="en">Specifies the city where a hotel (or its primary premises) is physically located.</rdfs:comment>
        <rdfs:label xml:lang="en">Hotel Is Located In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#restaurantIsLocatedIn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#restaurantIsLocatedIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Restaurant"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#City"/>
        <rdfs:comment xml:lang="en">Indicates the city where a restaurant is physically established.</rdfs:comment>
        <rdfs:label xml:lang="en">Restaurant Is Located In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#takesPlaceIn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#takesPlaceIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Event"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#City"/>
        <rdfs:comment xml:lang="en">Relates an event to the city where it occurs.</rdfs:comment>
        <rdfs:label xml:lang="en">Event Takes Place In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#touristAttractionIsLocatedIn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#touristAttractionIsLocatedIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#TouristAttraction"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#City"/>
        <rdfs:comment xml:lang="en">Relates a tourist attraction (e.g., museum, park) to the city where it is physically situated.</rdfs:comment>
        <rdfs:label xml:lang="en">Tourist Attraction Is Located In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#transportOperatesIn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#transportOperatesIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Transport"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#City"/>
        <rdfs:comment xml:lang="en">Links a transport service (e.g., metro, bus system) to the city where it operates.</rdfs:comment>
        <rdfs:label xml:lang="en">Transport Operates In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#averageMealPrice -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#averageMealPrice">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">The estimated average price for a standard meal (or per person) at this restaurant, in the local currency. Excludes special menus or discounts.</rdfs:comment>
        <rdfs:label xml:lang="en">Average meal price</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#averageTransportPrice -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#averageTransportPrice">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">The typical cost (e.g., per ride, kilometer, or hour) for using this transport service, in the local currency. Includes base fares or estimated averages.</rdfs:comment>
        <rdfs:label xml:lang="en">Average transport price</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#company -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#company">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Relates a transport service (e.g., bus line, bike-sharing system) to the company or organization responsible for its operation.</rdfs:comment>
        <rdfs:label xml:lang="en">Company</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#cuisineType -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#cuisineType">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Specifies the primary culinary style or cuisine served by the restaurant (e.g., Italian, Mexican, Vegan).</rdfs:comment>
        <rdfs:label xml:lang="en">Cuisine type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#eventDate -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#eventDate">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The date or date range when the event occurs.</rdfs:comment>
        <rdfs:label xml:lang="en">Event date</rdfs:label>
        <rdfs:sameAs>http://dbpedia.org/page/Date</rdfs:sameAs>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#hotelNightPrice -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#hotelNightPrice">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">he standard price for one night&apos;s accommodation at this hotel, in the local currency.</rdfs:comment>
        <rdfs:label xml:lang="en">Hotel nightly price</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#hotelStars -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#hotelStars">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">The official classification of a hotel&apos;s quality level, represented as a number of stars (typically 1-5).</rdfs:comment>
        <rdfs:label xml:lang="en">Hotel Stars</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#identifier -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#identifier">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#City"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Event"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Hotel"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Restaurant"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#TouristAttraction"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Transport"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">A unique machine-readable identifier for an entity (e.g., UUID, database key, or IRI). This is distinct from human-readable names and should be persistent and unambiguous.</rdfs:comment>
        <rdfs:label xml:lang="en">Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#latLong -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#latLong">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#City"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>A comma separated location (latitud, longitud)</rdfs:comment>
        <rdfs:label>Latitud Longitud</rdfs:label>
        <rdfs:sameAs>http://www.w3.org/2003/01/geo/wgs84_pos#lat_long</rdfs:sameAs>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#name -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#name">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#City"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Event"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Hotel"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Restaurant"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#TouristAttraction"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The primary human-readable identifier or title of an entity (e.g., city name, hotel name, event name).</rdfs:comment>
        <rdfs:label xml:lang="en">Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#rating -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#rating">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Hotel"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Restaurant"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#TouristAttraction"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">A numerical evaluation score representing the quality or user satisfaction of an entity (e.g., hotel, restaurant, attraction). Use xsd:decimal for fractional values (e.g., 4.7).</rdfs:comment>
        <rdfs:label xml:lang="en">Rating</rdfs:label>
        <rdfs:sameAs>https://schema.org/Rating#ratingValue</rdfs:sameAs>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Beach -->


    <owl:Class rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Beach">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#TouristAttraction"/>
        <rdfs:comment xml:lang="en">A sandy or pebbly shoreline by a body of water, used for swimming, sunbathing, or other recreational activities.</rdfs:comment>
        <rdfs:label>Beach</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#BeachHotel -->


    <owl:Class rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#BeachHotel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Hotel"/>
        <rdfs:comment xml:lang="en">A hotel located on or near a beach, offering direct access to the shoreline and often featuring water-related activities.</rdfs:comment>
        <rdfs:label xml:lang="en">Beach Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#BusinessHotel -->


    <owl:Class rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#BusinessHotel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Hotel"/>
        <rdfs:comment xml:lang="en">A hotel designed to accommodate business travelers, often featuring conference rooms, business centers, and convenient urban locations.</rdfs:comment>
        <rdfs:label xml:lang="en">Business Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#City -->


    <owl:Class rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#City">
        <rdfs:comment xml:lang="en">A large human settlement with dense population, infrastructure, and administrative functions, often a destination for tourism or business.</rdfs:comment>
        <rdfs:label xml:lang="en">City</rdfs:label>
        <rdfs:sameAs>https://dbpedia.org/page/City</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#CulturalAttraction -->


    <owl:Class rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#CulturalAttraction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#TouristAttraction"/>
        <rdfs:comment>A site or venue that showcases the arts, traditions, heritage, or customs of a community or region (e.g., theaters, festivals, historic districts).</rdfs:comment>
        <rdfs:label xml:lang="en">Cultural Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#CulturalEvent -->


    <owl:Class rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#CulturalEvent">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Event"/>
        <rdfs:comment xml:lang="en">A public gathering that showcases the traditions, arts, or heritage of a community, such as festivals, exhibitions, or performances.</rdfs:comment>
        <rdfs:label xml:lang="en">Cultural Event</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Event -->


    <owl:Class rdf:about="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#Event">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#takesPlaceIn"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/dfrison/ontologies/2025/3/onto-smart-tourism#City"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A planned public or social occasion, such as a sports competition, concert, festival, or exhibition.</rdfs:comment>
        <rdfs:label xml:lang="en">Event</rdfs:label>
    </owl:Class>
    


